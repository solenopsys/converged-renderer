{
  "name": "@solenopsys/converged-renderer",
  "description": "A high-performance renderer for building rich applications.",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx/runtime.js",
      "types": "./dist/jsx/runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx/runtime.js",
      "types": "./dist/jsx/runtime.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "jsx-runtime": [
        "./dist/jsx/runtime.d.ts"
      ],
      "jsx-dev-runtime": [
        "./dist/jsx/runtime.d.ts"
      ]
    }
  },
  "scripts": {
    "format": "bunx @biomejs/biome format src/* --write",
    "bundle": "bun build  src/index.ts --outfile dist/index.js --minify --external @solenopsys/converged-reactive ",
    "types": "bun dts-bundle-generator --no-check  --external-imports  @solenopsys/converged-reactive  --export-referenced-types -o dist/index.d.ts src/index.ts",
    "package": "bun run format && bun run bundle &&  bun run  types && npm publish --access public"
  },
  "dependencies": {
    "@solenopsys/converged-reactive": "^1.0.1",
    "dts-bundle-generator": "^9.3.1",
    "htm": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "typescript": "latest"
  }
}